.audio-view-rooter {
    height: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: space-between;
    box-sizing: border-box;
}
.button-view {
    width: 100%;
    display: flex;
    align-items: center;
    justify-content: space-around;
}
.button-image {
    width:60rpx;
    height:60rpx;
}
.image_frame {
    width:100%;
    height:100%;
    border-radius:50%
}
.progress-view {
    margin-top: 20rpx;
    margin-bottom: 20rpx;
    display: flex;
    justify-content: center;
    align-items: center
}
progress {
    width: 250px;
    margin-right: 10px
}