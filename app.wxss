/**app.wxss**/
.container {
    height: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: space-between;
    padding: 200rpx 0;
    box-sizing: border-box;
} 
.view-rooter {
    height: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: space-between;
    box-sizing: border-box;
}
.view-normal {
    width: 100%;
    display: flex;
    align-items: center;
    justify-content: space-around;
    padding: 20rpx;
}
input {
    border: 1px solid #ff00ff;
}
